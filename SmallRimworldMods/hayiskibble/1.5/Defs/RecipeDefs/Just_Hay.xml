<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <RecipeDef>
    <defName>Make_Kibble_With_Just_Hay</defName>
    <label>make kibble (hay only)</label>
    <description>Make animal kibble using only hay as ingredients.</description>
    <jobString>Making kibble.</jobString>
    <workAmount>350</workAmount>
    <workSpeedStat>CookSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_CookMeal</soundWorking>
    <allowMixingIngredients>false</allowMixingIngredients>
    <ingredientValueGetterClass>IngredientValueGetter_Nutrition</ingredientValueGetterClass>
    <ingredients>
      <li>
        <filter>
          <categories>
            <li>PlantFoodRaw</li>
          </categories>
          <thingDefs>
            <li>Hay</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <products>
      <Kibble>50</Kibble>
    </products>
    <fixedIngredientFilter>
     
      <thingDefs>
        <li>Hay</li>
      </thingDefs>
    </fixedIngredientFilter>
    <defaultIngredientFilter>
      <categories>
        <li>Foods</li>
      </categories>
      <disallowedCategories>
        <li>EggsFertilized</li>
      </disallowedCategories>
      <disallowedThingDefs>
        <li>InsectJelly</li>
      </disallowedThingDefs>
    </defaultIngredientFilter>
    <workSkill>Cooking</workSkill>
  </RecipeDef>

</Defs>
